    {
      "name": "e014136b",
      "displayName": "silver_orders_with_geo",
      "queryLines": [
        "SELECT \r\n",
        "  O.order_id,\r\n",
        "  O.order_timestamp,\r\n",
        "  C.address,\r\n",
        "  C.city,\r\n",
        "  C.state,\r\n",
        "  concat(\"US-\", c.state) as state_sub_code,\r\n",
        "  C.zip_code\r\n",
        "FROM orders_silver_pyspark O\r\n",
        "LEFT JOIN customers_silver_pyspark c \r\n",
        "ON O.customer_id = C.customer_id; "
      ],
      "catalog": "workspace",
      "schema": "sim_retail_demo"
    },
    {
      "name": "0ee8353e",
      "displayName": "gold_current_orders_pyspark",
      "queryLines": [
        "SELECT * FROM workspace.sim_retail_demo.gold_current_orders_pyspark"
      ]
    },
    {
      "name": "f81080e1",
      "displayName": "gold_current_orders_geo",
      "queryLines": [
        "SELECT \r\n",
        "  o.order_id,\r\n",
        "  o.customer_id,\r\n",
        "  c.name,\r\n",
        "  o.current_order_status,\r\n",
        "  o.current_status_timestamp,\r\n",
        "  o.order_placed_timestamp,\r\n",
        "  unix_timestamp(o.current_status_timestamp) - unix_timestamp(o.order_placed_timestamp) as fulfilment_time_sec,\r\n",
        "  c.city,\r\n",
        "  c.state,\r\n",
        "  c.zip_code\r\n",
        "FROM gold_current_orders_pyspark o\r\n",
        "LEFT JOIN customers_silver_pyspark c\r\n",
        "ON o.customer_id = c.customer_id;"
      ],
      "catalog": "workspace",
      "schema": "sim_retail_demo"
    }
  ],
